<PropertyList>

<path>heat.ac</path>

<animation>
<type>select</type>
<object-name>heat</object-name>
<condition>
<property>engines/engine[0]/running</property>
</condition>
</animation>

<animation>
<type>rotate</type>
<object-name>heat</object-name>
<property>/surface-positions/spoilers-pos-norm</property>
<interpolation>
<entry>
<ind>0.0</ind>
<dep>95</dep>
</entry>
<entry>
<ind>0.5</ind>
<dep>0</dep>
</entry>
<entry>
<ind>1.0</ind>
<dep>0</dep>
</entry>
</interpolation>
<center>
<x-m>4.5</x-m>
<y-m>0</y-m>
<z-m>0</z-m>
</center>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>material</type>
<object-name>heat</object-name>
<emission>
<red>255</red>
<green>255</green>
<blue>254</blue>
</emission>
</animation>

<animation>
<type>range</type>
<min-m>0</min-m>
<max-m>32000</max-m>
</animation>

</PropertyList>
