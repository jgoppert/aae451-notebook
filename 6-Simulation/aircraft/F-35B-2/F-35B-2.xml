  <flight_control>
      
    <property>ap/roll_hold</property>
    <property>ap/pitch_hold</property>
    <property>ap/yaw_hold</property>
    <property>ap/alt_hold</property>
    <property>ap/h-agl-cmd</property>

    <function name="ap/engine-roll-mix">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0.2
                100 0.2
                200 0
                300 0
            </tableData>
        </table>
    </function>

    <function name="ap/engine-pitch-mix">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0.6
                100 0.6
                200 0
                300 0
            </tableData>
        </table>
    </function>

    <function name="ap/aero-moment-mix">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0
                100 1
                200 1
                300 1
            </tableData>
        </table>
    </function>

    <function name="ap/engine-yaw-mix">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0
                100  0
                200 0
                300 0
            </tableData>
        </table>
    </function>

    <function name="ap/lift-fan-thrust-mix">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0.2
                100  0.2
                200 0.0
                300 0.0
            </tableData>
        </table>
    </function>

    <function name="ap/engine-thrust-mix">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   1
                100 1
                200 1.0
                300 1.0
            </tableData>
        </table>
    </function>
      
    <function name="ap/kp-roll">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   3
                100  3
                200 1
                300 1
            </tableData>
        </table>
    </function>

    <function name="ap/kd-roll">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   3
                100 3
                200 1
                300 1
            </tableData>
        </table>
    </function>

    <function name="ap/kp-pitch">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   3
                100 3
                200 10
                300 10
            </tableData>
        </table>
    </function>

    <function name="ap/kd-pitch">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   3
                100 3
                200 10
                300 10
            </tableData>
        </table>
    </function>


    <function name="ap/kp-yaw">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   1
                100 1
                200 1
                300 1
            </tableData>
        </table>
    </function>

    <function name="ap/kd-yaw">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   1
                100 1
                200 1
                300 1
            </tableData>
        </table>
    </function>
      

    <function name="ap/kp-alt">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0.001
                100 0.001
                200 0.001
                300 0.001
            </tableData>
        </table>
    </function>

    <function name="ap/kd-alt">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0.001
                100 0.001
                200 0.001
                300 0.001
            </tableData>
        </table>
    </function>
      
    <function name="ap/aileron-trim">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0
                100 0
            </tableData>
        </table>
    </function>

    <function name="ap/elevator-trim">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   1.2988612309031933
                100 1.3087562420014922
            </tableData>
        </table>
    </function>

    <function name="ap/rudder-trim">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0
                100 0
            </tableData>
        </table>
    </function>

    <function name="ap/throttle-trim">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0.9250074376220366
                100 0.933087257431862
            </tableData>
        </table>
    </function>

    <function name="ap/pitch-rad-trim">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0
                100 0
                200 0
                300 0
            </tableData>
        </table>
    </function>

    <function name="ap/thrust-angle-trim">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   1.570796349749601
                100 1.5537481223573382
                200 0
                300 0
            </tableData>
        </table>
    </function>
    
    <!-- just used to control animation in flightgear -->

    <function name="ap/spoiler-tbl-norm">
        <table>
            <independentVar>propulsion/engine/pitch-angle-rad</independentVar>
            <tableData>
                0  1
                1.57 0
            </tableData>
        </table>
    </function>
    <channel name="Spoiler">
      <pure_gain>
        <input>ap/spoiler-tbl-norm</input>
        <gain>1</gain>
        <output>fcs/spoiler-pos-norm</output>
      </pure_gain>
    </channel>

    <channel name="Gear">
      <pure_gain>
        <input>gear/gear-cmd-norm</input>
        <gain>1</gain>
        <output>gear/gear-pos-norm</output>
      </pure_gain>
    </channel>

    <channel name="Roll">
      <summer name="ap/roll-error">
        <value>0</value>
        <input>-attitude/phi-rad</input>
      </summer>
      <pid name="ap/roll-ap-error-pid">
        <input>ap/roll-error</input>
        <kp>ap/kp-roll</kp>
        <ki> 0 </ki>
        <kd>ap/kd-roll</kd>
        <clipto>
            <min>-0.3</min>
            <max>0.3</max>
        </clipto>
      </pid>
      <summer name="ap/roll-auto">
        <input>ap/aileron-trim</input>
        <input>ap/roll-ap-error-pid</input>
      </summer>
      <switch name="ap/aileron-cmd-norm">
        <default value="fcs/aileron-cmd-norm"/>
        <test value="ap/roll-auto">
          ap/roll_hold == 1
        </test>
      </switch>
    </channel>

    <channel name="Pitch">
      <summer name="ap/pitch-error">
        <input>ap/pitch-rad-trim</input>
        <input>-attitude/theta-rad</input>
      </summer>
      <pid name="ap/pitch-ap-error-pid">
        <input>ap/pitch-error</input>
        <kp>ap/kp-pitch</kp>
        <ki> 0 </ki>
        <kd>ap/kd-pitch</kd>
        <clipto>
            <min>-0.3</min>
            <max>0.3</max>
        </clipto>
      </pid>
      <summer name="ap/pitch-auto">
        <input>ap/elevator-trim</input>
        <input>ap/pitch-ap-error-pid</input>
      </summer>
      <switch name="ap/elevator-cmd-norm">
        <default value="fcs/elevator-cmd-norm"/>
        <test value="ap/pitch-auto">
          ap/pitch_hold == 1
        </test>
      </switch>
    </channel>

    <channel name="Yaw">
      <summer name="fcs/pitch-error">
        <value>270</value>
        <input>-attitude/psi-deg</input>
      </summer>
      <pid name="ap/yaw-ap-error-pid">
        <input>attitude/psi-rad</input>
        <kp>ap/kp-yaw</kp>
        <ki> 0 </ki>
        <kd>ap/kd-yaw</kd>
        <clipto>
            <min>-0.3</min>
            <max>0.3</max>
        </clipto>
      </pid>
      <summer name="ap/yaw-auto">
        <input>ap/rudder-trim</input>
        <input>ap/yaw-ap-error-pid</input>
      </summer>
      <switch name="ap/rudder-cmd-norm">
        <default value="fcs/elevator-cmd-norm"/>
        <test value="ap/yaw-auto">
          ap/yaw_hold == 1
        </test>
      </switch>
    </channel>

    <channel name="Altitude">
      <summer name="ap/alt-error">
        <input>ap/h-agl-cmd</input>
        <input>-position/h-agl-ft</input>
      </summer>
      <pid name="ap/alt-ap-error-pid">
        <input>ap/alt-error</input>
        <kp>ap/kp-alt</kp>
        <ki> 0 </ki>
        <kd>ap/kd-alt</kd>
        <clipto>
            <min>-0.6</min>
            <max>0.6</max>
        </clipto>
      </pid>
      <summer name="ap/alt-auto">
        <input>ap/throttle-trim</input>
        <input>ap/alt-ap-error-pid</input>
      </summer>
      <switch name="ap/throttle-cmd-norm">
        <default value="fcs/throttle-cmd-norm"/>
        <test value="ap/alt-auto">
          ap/alt_hold == 1
        </test>
      </switch>
    </channel>

    <channel name="Elevator">
      <pure_gain>
        <input>ap/elevator-cmd-norm</input>
        <gain>-ap/aero-moment-mix</gain>
        <output>fcs/elevator-pos-rad</output>
        <clipto>
            <min>-0.5</min>
            <max>0.5</max>
        </clipto>
      </pure_gain>
    </channel>

    <channel name="Left Aileron">
      <pure_gain>
        <input>ap/aileron-cmd-norm</input>
        <gain>ap/aero-moment-mix</gain>
        <output>fcs/left-aileron-pos-rad</output>
        <clipto>
            <min>-0.5</min>
            <max>0.5</max>
        </clipto>
      </pure_gain>
    </channel>

    <channel name="Right Aileron">
      <pure_gain>
        <input>ap/aileron-cmd-norm</input>
        <gain>-ap/aero-moment-mix</gain>
        <output>fcs/right-aileron-pos-rad</output>
        <clipto>
            <min>-0.5</min>
            <max>0.5</max>
        </clipto>
      </pure_gain>
    </channel>

    <channel name="Rudder">
      <pure_gain>
        <input>ap/rudder-cmd-norm</input>
        <gain>ap/aero-moment-mix</gain>
        <output>fcs/rudder-pos-rad</output>
        <clipto>
            <min>-0.5</min>
            <max>0.5</max>
        </clipto>
      </pure_gain>
    </channel>
      
    <channel name="Engine0">
      <pure_gain name="ap/elevator_to_engine0">
        <input>ap/elevator-cmd-norm</input>
        <gain>-ap/engine-pitch-mix</gain>
      </pure_gain>
      <pure_gain name="ap/throttle_to_engine0">
        <input>ap/throttle-cmd-norm</input>
        <gain>ap/engine-thrust-mix</gain>
      </pure_gain>
      <summer>
        <input>ap/elevator_to_engine0</input>
        <input>ap/throttle_to_engine0</input>
        <output>ap/throttle-pos-norm</output>
        <clipto>
            <min>0</min>
            <max>2</max>
        </clipto>
      </summer>
    </channel>

    <channel name="Engine0_Yaw">
      <!-- yawing engine doesn't seem to do anything in JSBSim, so no yaw control here currently-->
      <pure_gain name="ap/rudder_to_engine0_yaw">
        <input>ap/rudder-cmd-norm</input>
        <gain>ap/engine-yaw-mix</gain>
        <output>propulsion/engine/yaw-angle-rad</output>
        <clipto>
            <min>-0.1</min>
            <max>0.1</max>
        </clipto>
      </pure_gain>
    </channel>

    <channel name="Engine1">
      <pure_gain name="ap/elevator_to_engine1">
        <input>ap/elevator-cmd-norm</input>
        <gain>ap/engine-pitch-mix</gain>
      </pure_gain>
      <pure_gain name="ap/throttle_to_engine1">
        <input>ap/throttle-cmd-norm</input>
        <gain>ap/lift-fan-thrust-mix</gain>
      </pure_gain>
      <summer>
        <input>ap/elevator_to_engine1</input>
        <input>ap/throttle_to_engine1</input>
        <output>fcs/throttle-pos-norm[1]</output>
        <clipto>
            <min>0</min>
            <max>1</max>
        </clipto>
      </summer>
    </channel>

    <channel name="Engine2">
      <pure_gain name="ap/aileron_to_engine2">
        <input>ap/aileron-cmd-norm</input>
        <gain>-ap/engine-roll-mix</gain>
      </pure_gain>
      <pure_gain name="ap/throttle_to_engine2">
        <input>ap/throttle-cmd-norm</input>
        <gain>ap/lift-fan-thrust-mix</gain>
      </pure_gain>
      <summer>
        <input>ap/aileron_to_engine2</input>
        <input>ap/throttle_to_engine2</input>
        <output>fcs/throttle-pos-norm[2]</output>
        <clipto>
            <min>0</min>
            <max>1</max>
        </clipto>
      </summer>
    </channel>

    <channel name="Engine3">
      <pure_gain name="ap/aileron_to_engine3">
        <input>ap/aileron-cmd-norm</input>
        <gain>ap/engine-roll-mix</gain>
      </pure_gain>
      <pure_gain name="ap/throttle_to_engine3">
        <input>ap/throttle-cmd-norm</input>
        <gain>ap/lift-fan-thrust-mix</gain>
      </pure_gain>
      <summer>
        <input>ap/aileron_to_engine3</input>
        <input>ap/throttle_to_engine3</input>
        <output>fcs/throttle-pos-norm[3]</output>
        <clipto>
            <min>0</min>
            <max>1</max>
        </clipto>
      </summer>
    </channel>


  </flight_control>

